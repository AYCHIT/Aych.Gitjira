    "atlassian-jwt": "^0.1.5",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bottleneck": "2.9.0",
    "bull": "^3.4.7",
    "bunyan-sentry-stream": "^1.2.1",
    "cookie-session": "^2.0.0-beta.3",
    "csurf": "^1.9.0",
    "date-fns": "^1.29.0",
    "express": "^4.16.3",
    "express-sslify": "^1.2.0",
    "helmet": "^3.13.0",
    "markdown": "^0.5.0",
    "moo": "^0.5.0",
    "newrelic": "^4.13.1",
    "pg": "^7.4.3",
    "primer": "^10.6.0",
    "probot": "^7.5.2",
    "probot-config": "^0.1.0",
    "request": "^2.88.0",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^4.0.0",
    "sequelize-encrypted": "^1.0.0"
  },
  "devDependencies": {
    "@types/bull": "^3.3.19",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.6.0",
    "keygrip": "^1.0.2",
    "nock": "^9.3.2",
    "nodemon": "^1.17.2",
    "react": "^16.6.0",
    "smee-client": "^1.0.1",
    "standard": "^11.0.1",
    "supertest": "^3.3.0",
    "testdouble": "^3.9.1",
    "testdouble-jest": "^2.0.0",
    "testdouble-nock": "^0.2.0"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "./test/setup",
    "globalTeardown": "./test/teardown"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ],
    "global": [
      "td",
      "app",
      "nock"
    ]
  }
}
